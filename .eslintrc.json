{
    "extends": "standard",
    "rules": {
      "require-jsdoc": ["off", {
          "require": {
              "FunctionDeclaration": false,
              "MethodDefinition": false,
              "ClassDeclaration": false
            }
          }],
      "comma-dangle": [0, "always-multiline"],
      "no-trailing-spaces": 0
    }
}
